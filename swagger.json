      "CreateProductSpuDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Áo thun hàn quốc"
          },
          "thumb": {
            "type": "string",
            "example": "https://example.com/image.jpg"
          },
          "description": {
            "type": "string",
            "example": "Áo thun hàn quốc hiểu dáng Regular"
          },
          "price": {
            "type": "number",
            "example": 100000
          },
          "category": {
            "example": [
              100001,
              100002,
              100003
            ],
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "attributes": {
            "description": "Danh sách thuộc tính của sản phẩm",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ProductSpuAttributeDto"
            }
          },
          "quantity": {
            "type": "number",
            "example": 100,
            "description": "Số lượng tổng của sản phẩm"
          },
          "variations": {
            "description": "Danh sách các biến thể của sản phẩm",
            "example": [
              {
                "images": [],
                "name": "Color",
                "options": [
                  "Red",
                  "Green"
                ]
              },
              {
                "images": [],
                "name": "Size",
                "options": [
                  "S",
                  "M"
                ]
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ProductSpuVariationDto"
            }
          },
          "sku_list": {
            "example": [
              {
                "sku_tier_idx": [
                  0,
                  0
                ],
                "sku_price": "100000",
                "sku_stock": 10
              },
              {
                "sku_tier_idx": [
                  0,
                  1
                ],
                "sku_price": "110000",
                "sku_stock": 15
              },
              {
                "sku_tier_idx": [
                  1,
                  0
                ],
                "sku_price": "120000",
                "sku_stock": 20
              },
              {
                "sku_tier_idx": [
                  1,
                  1
                ],
                "sku_price": "130000",
                "sku_stock": 25
              }
            ],
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ProductSpuSkuListDto"
            }
          }
        },
        "required": [
          "name",
          "thumb",
          "description",
          "price",
          "category",
          "attributes",
          "quantity",
          "variations",
          "sku_list"
        ]
      },
