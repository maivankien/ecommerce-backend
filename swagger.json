      "Grant": {
        "type": "object",
        "properties": {
          "resource": {
            "type": "string",
            "example": "resourceId"
          },
          "actions": {
            "description": "List of actions permitted by this grant",
            "example": [
              "read:any",
              "update:any",
              "delete:any"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "attributes": {
            "type": "string",
            "description": "Attributes that can be accessed with this grant",
            "example": "*"
          }
        },
        "required": [
          "resource",
          "actions",
          "attributes"
        ]
      },
      "CreateRoleDto": {
        "type": "object",
